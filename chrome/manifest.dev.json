{
  "version": "0.0.0",
  "name": "react-chrome-extension-example",
  "manifest_version": 2,
  "description": "Example for react-gulp-chrome-extension-boilerplate",
  "browser_action": {
    "default_title": "React Gulp Chrome Extension Example",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [ "contextMenus", "management", "tabs", "storage", "https://github.com/*" ],
  "content_security_policy": "default-src 'self'; style-src * 'unsafe-inline' 'self' blob:; img-src 'self' data:;"
}
